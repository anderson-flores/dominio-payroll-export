{
  "name": "convenia/dominio-payroll-export",
  "description": "Format payroll data to import to Dominio Software",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "php",
    "dominio",
    "folha de pagamento"
  ],
  "authors": [
      {
          "name": "Eduardo Bizarro",
          "email": "edbizarro@gmail.com"
      },
      {
        "name": "Convenia",
        "email": "contato@convenia.com.br"
      }
  ],
  "require": {
    "php": ">=7.1",
    "danielstjules/stringy": "^2.3",
    "convenia/convenia-recitable": "1.5.0"
  },
  "require-dev": {
      "phpunit/phpunit": "^4.0|^5.0",
      "fabpot/php-cs-fixer": "^1.11",
      "mockery/mockery": "^0.9.4"
  },
  "repositories": [
      {
          "type": "git",
          "url": "https://github.com/convenia/convenia-recitable.git"
      }
  ],
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "Convenia\\Dominio\\PayrollExport\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Convenia\\Dominio\\PayrollExport\\Tests\\": "tests/PayrollExport/"
    }
  },
  "config":{
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
